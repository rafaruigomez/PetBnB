<h1>Flats#new</h1>
<p>Find me in app/views/flats/new.html.erb</p>
<%= simple_form_for(@flat) do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :address %>
    <%= f.input :price %>
    <%= f.input :location %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>

    <%= f.submit %>
  <% end %>
